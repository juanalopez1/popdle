{
    "transforms": [{
        "mode": "sql",
        "include": "**/*.sql",
        "emitTemplate": "{{dir}}/{{name}}.queries.ts"
    }],
    "srcDir": "./src/queries/",
    "failOnError": true,
    "camelCaseColumnNames": true,
    "typesOverrides": {
        "username_domain": "string",
        "email_domain": "string"
    }
}
